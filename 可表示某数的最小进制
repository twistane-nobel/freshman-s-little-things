#include <stdio.h>
#include <string.h>
#include <ctype.h>
int getMinBase(){
	char str[11];
	scanf("%s",str);
	int len=strlen(str);
	int max=0;
	for(int i=0;i<len;i++){
		int digit;
		if(isdigit(str[i])){
			digit=str[i]-'0';
		}else{
			digit=str[i]-'a'+10;
		}
		if(digit>max){
			max=digit;
		}
	}
	int min=max+1;
	if(min<2){
		min=2;
	}
	return min;
}
int main(){
	printf("%d",getMinBase());
	return 0;
}
