#include <stdio.h>
int main(){
	int N;
	scanf("%d",&N);
	long long a;
	for (long long i=0;i<N;i++){
		scanf("%lld",&a);
		if (a==1){
			printf("No\n");
		}else if(a==2){
			printf("Yes\n");
		}else{
			long long j;
			int m=1;
			for (j=2;j*j<=a;j++){
				long long r=a%j;
				if(r==0){
					printf("No\n");
					m=0;
					break;
				}
			}
			if(m==1){
				printf("Yes\n");
			}
		}
	}
    return 0;
}
