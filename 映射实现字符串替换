#include <stdio.h>
#include <string.h>
int main(){
	long long n;
	scanf("%lld",&n);
	getchar();
	char S[200005];
	scanf("%s",S);
	long long q;
	scanf("%lld",&q);
	getchar();
	char m[26];
	for(int i=0;i<26;i++){
		m[i]=i+'a';
	}
	char c,d;
	for(long long i=0;i<q;i++){
		scanf("%c %c",&c,&d);
		getchar();
		for(int j=0;j<26;j++){
			if(m[j]==c){
				m[j]=d;
			}
		}
	}
	for(long long i=0;i<n;i++){
		S[i]=m[S[i]-'a'];
	}
	printf("%s",S);
	return 0;
} 
