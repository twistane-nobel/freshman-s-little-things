#include <stdio.h>
int num(int n,int arr[]){
	for (int i=0;i<n;i++){
		arr[i]=1;
    }
    int count=0;
	int remain=n;
	int index=0;
	while(remain>1){
		if (arr[index]==1){
			count++;
			if(count==3){
			    arr[index]=0;
			    remain--;
			    count=0;
			}
		}
		index=(index+1)%n;
	}
	
	for(int i=0;i<n;i++){
		if(arr[i]==1){
			printf("%d\n",i+1);
			break;
		}
	}
}
int main(){
	int n;
	scanf("%d",&n);
	int arr[n];
	num(n,arr);
    return 0;
}
